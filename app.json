{
  "expo": {
    "name": "TMLSN",
    "slug": "tmlsn-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "tmlsn",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F3031"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tmlsn.app",
      "infoPlist": {
        "NSCameraUsageDescription": "TMLSN needs camera access to log your meals via photo.",
        "NSPhotoLibraryUsageDescription": "TMLSN needs photo library access to select meal photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2F3031"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ],
      "package": "com.tmlsn.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2F3031"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
