# API Keys and Secrets
# Copy this file to .env.local and fill in your actual values

# Supabase (required for login/create account and cloud storage)
# Create a project at https://supabase.com, then run supabase/migrations/001_user_data.sql in the SQL Editor
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# USDA FoodData Central (1,000 req/hour with real key; free at https://api.data.gov/signup/)
EXPO_PUBLIC_USDA_API_KEY=your_usda_api_key_here

# Food Recognition API (e.g., Clarifai, Google Vision, or similar)
FOOD_RECOGNITION_API_KEY=your_api_key_here
FOOD_RECOGNITION_API_URL=https://api.example.com/v1/food-recognition

# Push Notification Service (if using external service)
EXPO_PUSH_TOKEN=your_expo_push_token_here

# Newsletter/Content API (for notifications about new content)
CONTENT_API_KEY=your_content_api_key_here
CONTENT_API_URL=https://api.example.com/v1/content

# Analytics (optional)
ANALYTICS_KEY=your_analytics_key_here
